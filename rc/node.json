{
  "$schema": "https://json.schemastore.org/eslintrc",
  "extends": ["plugin:n/recommended"],
  "env": { "node": true },
  "rules": {
    "n/no-missing-import": "off"
  },
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.d.ts"],
      "rules": {
        "n/no-unsupported-features/es-syntax": "off"
      }
    }
  ]
}
